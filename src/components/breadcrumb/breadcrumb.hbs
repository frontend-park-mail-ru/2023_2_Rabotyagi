<div class="breadcrumb">
    {{#each items}}
        {{#if this.isDelimiter}}
            <span class="text-regular">{{this.text}}</span>
        {{else}}
            <button id="{{this.id}}"><span class="text-regular">{{this.text}}</span></button>
        {{/if}}
    {{/each}}
