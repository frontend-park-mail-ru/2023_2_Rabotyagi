{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Simple Text",
  "main": "index.js",
  "scripts": {
    "\n============Run dev=======================\n": "",
    "dev-win": "start npm run build-dev && start npm run server",
    "dev-linux": "npm run build-dev & npm run server",
    "\n============Start server==================\n": "",
    "server": "node server/index.mjs",
    "\n============Build bundles=================\n": "",
    "build-dev": "webpack --node-env development --mode development",
    "build-prod": "webpack --node-env production --mode production",
    "\n============Build Docker Image============\n": "",
    "build": "docker build . -t nikdemi/rabotyagi-frontend:latest",
    "\n============Build Docs====================\n": "",
    "predocs": "rmdir -r -f ./docs",
    "docs": "jsdoc .\\src\\ --recurse -d .\\docs -c .\\jsdoc.json",
    "\n============Linting=======================\n": "",
    "lint": "npx eslint src"
  },
