<div class="carousel">
    <div id="selectPrev"></div>
    {{#if this}}
        {{#each this}}
            <div class="carousel__item">
                <img src="{{this.url}}" data-id="{{this.id}}" />
            </div>
        {{/each}}
        
    {{else}}
        <div class="placeholder" style="display: flex; width: 100%; height: 100%; background-color: darkgrey;"></div>
    {{/if}}
    <div id="selectNext"></div>
</div>