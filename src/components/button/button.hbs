<button 
{{#if id}} id="{{this.id}}" {{/if}}  
{{#if variant}} class="{{this.variant}}{{#if subVariant}} {{subVariant}}{{/if}}" {{/if}}
{{#if style}}
    style="{{this.style}}"
{{/if}}>
    <div id="leftIcon"></div>
    <span style="text-align: center; text-wrap: nowrap;" {{#if text.class}}class="{{this.text.class}}"{{/if}}>{{this.text.content}}</span>
    <div id="rightIcon"></div>
</button>